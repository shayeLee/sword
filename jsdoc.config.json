{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
    "include": ["./src"],
		"exclude": ["src/index.js"]
	},
	"plugins": ["plugins/markdown"],
	"markdown": {
		"tags": [],
		"excludeTags": [],
		"hardwrap": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./doc/",
		"recurse": true,
		"readme": "README.md"
	}
}
